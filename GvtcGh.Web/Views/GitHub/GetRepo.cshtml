@using System.Reflection
@model IList<dynamic>
@{
    ViewBag.Title = "Get Repository";
}

<h2>GetRepo</h2>
<p>
    @Html.ActionLink("Back", "Index")
</p>
<hr />
<table>
    @if (Model.Count == 0)
    {
        <tr><td>Repository Not Found</td></tr>
    }
    else
    {
        foreach (var item in Model)
        {
            <tr><td>&nbsp;</td></tr>
            foreach (var prop in item as IDictionary<string, object>)
            {
                <tr>
                    <td>
                        @prop.Key:&nbsp;
                    </td>
                    <td>
                        @prop.Value
                    </td>
                </tr>
            }
        }
    }

</table>

<p>
    @Html.ActionLink("Back", "Index")
</p>